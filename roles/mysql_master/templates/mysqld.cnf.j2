[mysqld]
server-id               = 1
log_bin                 = /var/log/mysql/mysql-bin.log
log_error               = /var/log/mysql/error.log
max_binlog_size   = 100M
binlog_format = row
binlog_ignore_db        = mysql
bind-address            = 0.0.0.0
mysqlx-bind-address     = 127.0.0.1
gtid-mode=ON
enforce-gtid-consistency
log-replica-updates